                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionOpp.allOppUSer}')).checked == true) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionOpp.allOppUSer}')).checked = false;
                }
            }
                        

            function allOnCaseUSer() {
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionCase.allCaseUSer}')).checked == true) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionCase.allCaseUSer}')).checked = false;
                }
            }
          
        function uncheckOther(elm) {
            document.getElementById('{!$Component.thePageBlockOne.Hourlydistribution}').checked=false;
            document.getElementById('{!$Component.thePageBlockOne.Dailydistribution}').checked=false;
            document.getElementById('{!$Component.thePageBlockOne.weeklydistribution}').checked=false;
            document.getElementById('{!$Component.thePageBlockOne.monthlydistribution}').checked=false;
            elm.checked=true;
            
            if (document.getElementById(('{!$Component.thePageBlockOne.weeklydistribution}')).checked == true){
                document.getElementById(('{!$Component.theForm.thePageBlockOne.weeklydistribution}')).disabled = true ;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.monthlydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Dailydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Hourlydistribution}')).disabled = false;
            }
            else if(document.getElementById(('{!$Component.thePageBlockOne.monthlydistribution}')).checked == true){
                document.getElementById(('{!$Component.theForm.thePageBlockOne.monthlydistribution}')).disabled = true ;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.weeklydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Dailydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Hourlydistribution}')).disabled = false;
            }
            else if(document.getElementById(('{!$Component.thePageBlockOne.Dailydistribution}')).checked == true){
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Dailydistribution}')).disabled = true ;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.weeklydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.monthlydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Hourlydistribution}')).disabled = false;
            }
            else if(document.getElementById(('{!$Component.thePageBlockOne.Hourlydistribution}')).checked == true){
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Hourlydistribution}')).disabled = true;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.Dailydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.weeklydistribution}')).disabled = false;
                document.getElementById(('{!$Component.theForm.thePageBlockOne.monthlydistribution}')).disabled = false;
            }

            
        }
    
    //--------------Automated Distribution [RRD-227] Start--------------------  
        function uncheckOtherAutomatedDistribution(elm) {
            if(elm.checked==true){
                document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.DistributeHourly}').checked=false;
                document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.DistributeDaily}').checked=false;
                document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.SkipWeekends}').checked=false;
                document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.SkipWeekends}').disabled=false;
                elm.checked = true
            }
            else if(elm.checked==false){
                elm.checked = false;
                document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.SkipWeekends}').disabled=true;
                document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.SkipWeekends}').checked=false;
            }
        }
    //--------------Automated Distribution [RRD-227] End--------------------   
    
    
