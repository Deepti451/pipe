                document.getElementById('{!$Component.thePageBlockOne.thepageblockbuttonsbottom.saveandnextbottom}').style.display = 'InLine';
                document.getElementById('{!$Component.thePageBlockOne.thepageblockbuttonsbottom.cancelbottom}').style.display = 'InLine';
                document.getElementById('{!$Component.thePageBlockOne.thepageblockbuttonsbottom.editbottom}').style.display = 'None';
                document.getElementById('{!$Component.thePageBlockOne.thepageblockbuttonsbottom.nextstepbottom}').style.display = 'None';
                document.getElementById("hideDiv").style.display = "inline";
                document.getElementById("showDiv").style.display = "none";
                document.getElementById("hideDivP").style.display = "inline";
                document.getElementById("showDivP").style.display = "none"; 
                document.getElementById("hideDivC").style.display = "inline";
                document.getElementById("showDivC").style.display = "none";
                document.getElementById("hideDivOpp").style.display = "inline";
                document.getElementById("showDivOpp").style.display = "none";                   
                
                document.getElementById("hideDiv").style.display = "inline";
                document.getElementById("showDiv").style.display = "none";
                document.getElementById("hideCaseDiv").style.display = "inline";
                document.getElementById("showCaseDiv").style.display = "none";
                
                document.getElementById("hideDiv").style.display = "inline";
                document.getElementById("showDiv").style.display = "none";
                document.getElementById("hideAccDiv").style.display = "inline";
                document.getElementById("showAccDiv").style.display = "none";
                
                document.getElementById("hideOppDiv").style.display = "inline";
                document.getElementById("showOppDiv").style.display = "none";
                
                document.getElementById("hideLeadNotification").style.display = "inline";
                document.getElementById("showLeadNotification").style.display = "none";
                document.getElementById("hideAccNotification").style.display = "inline";
                document.getElementById("showAccNotification").style.display = "none";
                document.getElementById("hideOppNotification").style.display = "inline";
                document.getElementById("showOppNotification").style.display = "none";
                document.getElementById("cyclehideDiv").style.display = "inline";
                document.getElementById("cycleshowDiv").style.display = "none";
                document.getElementById("hideCaseNotification").style.display = "inline";
                document.getElementById("showCaseNotification").style.display = "none";
                
                document.getElementById("defaultUserO").style.display = "inline";
                document.getElementById("defaultUser").style.display = "none";
                
                //--------------Automated Distribution [RRD-227] Start--------------------
                document.getElementById("hideAutomatedDistribution").style.display = "inline";
                document.getElementById("showAutomatedDistribution").style.display = "none";
                if(document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.DistributeHourly}').checked == false && 
                document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.DistributeDaily}').checked == false){
                    document.getElementById('{!$Component.thePageBlockOne.hidepgaeblocksectionAutomatedDistribution.SkipWeekends}').disabled = true;
                }
                //--------------Automated Distribution [RRD-227] End--------------------
                
                prevOwner();
                prevOwnerCase();
                prevOwnerOpp();
            }

            function allLead() {
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksection.allLeadOnlineUSer}')).checked == true) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksection.allLeadOnlineUSer}')).checked = false;
                }
            }
            /////////////////////////Sunaiyana///////////////////////
            function prevOwner() {
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionP.allLeadOnlineUSer}')).checked == true) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionP.allLeadOnlineUSer}')).checked = true;
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionP.leadSelect}')).disabled = false;
                }
                
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionP.allLeadOnlineUSer}')).checked == false) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionP.allLeadOnlineUSer}')).checked = false;
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionP.leadSelect}')).disabled = true;
                }

            }
            function prevOwnerCase() {
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionC.allLeadOnlineUSer}')).checked == true) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionC.allLeadOnlineUSer}')).checked = true;
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionC.caseSelect}')).disabled = false;
                }
                
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionC.allLeadOnlineUSer}')).checked == false) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionC.allLeadOnlineUSer}')).checked = false;
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionC.caseSelect}')).disabled = true;
                }

            }
            function prevOwnerOpp() {
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionO.allOppOnlineUSer}')).checked == true) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionO.allOppOnlineUSer}')).checked = true;
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionO.oppSelect}')).disabled = false;
                }
                
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionO.allOppOnlineUSer}')).checked == false) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionO.allOppOnlineUSer}')).checked = false;
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksectionO.oppSelect}')).disabled = true;
                }

            }
            ////////////////////////////////////////////
            function allOnlineUSer() {
                if (document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksection.allLeadUSer}')).checked == true) {
                    document.getElementById(('{!$Component.thePageBlockOne.hidepgaeblocksection.allLeadUSer}')).checked = false;
                }
