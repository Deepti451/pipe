        <linkType>javascript</linkType>
        <masterLabel>Synchronize with MailChimp</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var ChimpSyncNamespace = &apos;ChimpSync__&apos;;

var epochNow = (new Date().getTime())+&apos;&apos;;
var fancyboxJSScript = document.createElement(&apos;script&apos;);
fancyboxJSScript.type  = &quot;text/javascript&quot;;
fancyboxJSScript.src = &apos;/resource/&apos;+epochNow +&apos;/&apos;+ChimpSyncNamespace+&apos;FancyboxJS&apos;;
document.getElementsByTagName(&apos;head&apos;)[0].appendChild(fancyboxJSScript );

//code for custom alert
var ChimpSyncBox = {
	campaign_segmentation : &apos;{!JSENCODE(JSINHTMLENCODE(Campaign.ChimpSync__Segmentation__c))}&apos;,
	campaignName : &apos;{!JSENCODE(JSINHTMLENCODE(Campaign.Name))}&apos;,
	dateCreated : &apos;{!JSENCODE(JSINHTMLENCODE(TEXT(TODAY())))}&apos;,
	mc_campaign_id : &apos;{!JSENCODE(JSINHTMLENCODE(Campaign.ChimpSync__mailchimp_campaign_id__c))}&apos;,
	SF_campaign_id : &apos;{!Campaign.Id}&apos;,
	orgid : &apos;{!Organization.Id}&apos;,
	subscriberActivity : (&apos;{!Campaign.ChimpSync__Sync_Member_Activity__c}&apos; == 1) ? &apos;true&apos; : &apos;false&apos;,
	userEmail : &apos;{!JSENCODE(JSINHTMLENCODE($User.Email))}&apos;,
	mailchimpList : &apos;{!JSENCODE(JSINHTMLENCODE(Campaign.ChimpSync__MailChimp_List__c))}&apos;.replace(/&apos;/g, &quot;\\&apos;&quot;),
	ChimpSync_textStart : &apos;&lt;b&gt;What do you want to do?&lt;/b&gt;&apos;,
	
    render : function(){		
		
		var head = document.head || document.getElementsByTagName(&apos;head&apos;)[0];
		var style = document.createElement(&apos;style&apos;);

		style.type = &apos;text/css&apos;;
		if (style.styleSheet){
		  style.styleSheet.cssText = ChimpSyncBox.styleHTML;
		} else {
		  style.appendChild(document.createTextNode(ChimpSyncBox.styleHTML));
		}

		head.appendChild(style);
		
		var dialog = &apos;&lt;div style=&quot;text-align: left;&quot;&gt;&apos;;	
		
		dialog += &apos;&lt;div id=&quot;CS_info&quot;&gt;ChimpSync has started ...&lt;/div&gt;&lt;div id=&quot;CS_outer&quot;&gt;&lt;div class=&quot;CS_icon_box&quot;&gt;&lt;img src=&quot;/resource/&apos;+ChimpSyncNamespace+&apos;FancyboxIM/question.png&quot; /&gt;&lt;/div&gt;&lt;div style=&quot;margin:10px 0;&quot;&gt;&apos;+
			&apos;&lt;b&gt;What do you want to do?&lt;/b&gt;&apos;+
		&apos;&lt;/div&gt;&apos;+
		&apos;&lt;div&gt;&apos;;
		
		if (!this.mc_campaign_id) {
			dialog += this.createCampaignHTML + this.syncReportsHTML;
		} else {
			dialog += this.createCampaignHTML + this.updateCampaignHTML + this.syncReportsHTML;
		}
		
		dialog += &apos;&lt;div style=&quot;text-align: center;padding-top: 13px;&quot;&gt;&apos; +
		&apos;&lt;button style=&quot;padding: 3px 13px;background: #c23934;color:white; border: 1px solid #c23934;&quot; onclick=&quot;window.location.reload();&quot;&gt;Close&lt;/button&gt;&apos;+
		&apos;&lt;button style=&quot;padding: 3px 13px;&quot; onclick=&quot;window.ChimpSync.continue(\&apos;&apos; + this.SF_campaign_id + &apos;\&apos;,\&apos;&apos; + this.orgid + &apos;\&apos;,\&apos;&apos; + this.mc_campaign_id + &apos;\&apos;,\&apos;&apos; + this.campaignName + &apos;\&apos;,\&apos;&apos; + this.dateCreated + &apos;\&apos;,\&apos;&apos; + this.userEmail + &apos;\&apos;,\&apos;&apos; + this.mailchimpList + &apos;\&apos;,\&apos;&apos; + this.campaign_segmentation + &apos;\&apos;,\&apos;&apos; + this.subscriberActivity + &apos;\&apos;);&quot;&gt;Start&lt;/button&gt;&apos; +
		&apos;&lt;/div&gt;&lt;/div&gt;&apos;;
		
		dialog += &apos;&lt;/div&gt;&lt;div id=&quot;CS_progress&quot;&gt;&lt;/div&gt;&lt;div class=&quot;CS_errormessage&quot;&gt;&lt;a href=&quot;http://chimpsync.com/support/#sync&quot; target=&quot;blank&quot;&gt;&lt;b&gt;How to Sync?&lt;/b&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;&apos;;
        var winW = window.innerWidth;
        var winH = window.innerHeight;
        var dialogoverlay = document.getElementById(&apos;dialogoverlay&apos;);
        var dialogbox = document.getElementById(&apos;dialogbox&apos;);
        dialogoverlay.style.display = &quot;block&quot;;
        dialogoverlay.style.height = winH+&quot;px&quot;;
        dialogbox.style.left = (winW/2) - (425 * .5)+&quot;px&quot;;
        dialogbox.style.top = &quot;100px&quot;;
		dialogbox.style.display = &quot;block&quot;;
        document.getElementById(&apos;dialogboxbody&apos;).innerHTML = dialog;    
    },
	styleHTML: &apos;.CS_icon,.CS_text{float:left}#CS_info,.CS_errormessage_email{font-weight:700}#CS_progress{width:31px;margin:10px auto}&apos;+
	&apos;.CS_icon_box{width:64px;height:85px;float:left}.CS_text{width:190px}#CS_message{margin-top:20px}.CS_plan,.cs_choice{margin:3px 0}&apos;+
	&apos;.CS_errormessage{font-size:13px;border-top:1px solid #d3d3d3;padding-top:10px;text-align:center}.cs_choice{display:inline-block}.CS_clear{clear:both}&apos;+
	&apos;.CS_new_version.CS_css{border-top:1px solid #d3d3d3;margin-bottom:12px;color:green;font-weight:700;border-bottom:1px solid #d3d3d3;margin-top:-15px;padding:6px 0}&apos;+
	&apos;#dialogoverlay{ 	display: none; 	opacity: .2; background-color:black;	position: fixed; 	top: 0px; 	left: 0px; 	width: 100%; 	&apos;+
	&apos;z-index: 9999; } #dialogbox{ 	display: none; 	position: fixed; 	background: white; 	border-radius:7px;  	width:425px; 	z-index: 9999; &apos;+
	&apos;+background-color: #fff; -webkit-background-clip: padding-box; background-clip: padding-box; border: 1px solid #999; border: 1px solid rgba(0,0,0,.2); &apos;+
	&apos;border-radius: 6px; outline: 0; -webkit-box-shadow: 0 3px 9px rgba(0,0,0,.5); box-shadow: 0 3px 9px rgba(0,0,0,.5); }&apos;,

	initialHTML : &apos;&lt;div id=&quot;dialog&quot;&gt;&lt;div id=&quot;dialogoverlay&quot;&gt;&lt;/div&gt;&lt;div id=&quot;dialogbox&quot;&gt;&lt;div id=&quot;dialogboxbody&quot; style=&quot;padding:20px;&quot;&gt;&lt;/div&gt;&apos;+
	&apos;&lt;/div&gt;&lt;/div&gt;&apos;
	,
	createCampaignHTML: &apos;&lt;span class=&quot;cs_choice&quot;&gt;&lt;input type=&quot;radio&quot; name=&quot;sync_type&quot; value=&quot;new&quot; id=&quot;new_cs&quot;&gt;&lt;label for=&quot;new_cs&quot;&gt;&lt;b&gt;Create new campaign&lt;/b&gt; in MailChimp&lt;/label&gt;&lt;/span&gt;&lt;br /&gt;&apos;,
	updateCampaignHTML : &apos;&lt;span class=&quot;cs_choice&quot;&gt;&lt;input type=&quot;radio&quot; name=&quot;sync_type&quot; value=&quot;update&quot; id=&quot;update_cs&quot;&gt;&lt;label for=&quot;update_cs&quot;&gt;&lt;b&gt;Update members&lt;/b&gt; on MailChimp campaign&lt;/label&gt;&lt;/span&gt;&lt;br/&gt;&apos;,
	syncReportsHTML :&apos;&lt;span class=&quot;cs_choice&quot;&gt;&lt;input type=&quot;radio&quot; name=&quot;sync_type&quot; value=&quot;update_reports&quot; id=&quot;update_all_cs&quot;&gt;&lt;label for=&quot;update_all_cs&quot;&gt;Sync &lt;b&gt;campaign reports&lt;/b&gt;&lt;/label&gt;&lt;/span&gt;&apos;

}


var bodyElement = document.getElementsByTagName(&quot;BODY&quot;)[0];
bodyElement.insertAdjacentHTML(&apos;beforeend&apos;, ChimpSyncBox.initialHTML );

var startTimeout = false;
ChimpSyncBox.render();</url>
