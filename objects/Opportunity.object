        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Re-assign Using RRD</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/30.0/connection.js&quot;)} 
{!requireScript(&quot;/soap/ajax/30.0/apex.js&quot;)} 

var retStr; 
var checkExpiry = sforce.apex.execute(&quot;rrde.AssignSignleRecordctrlr&quot;, &quot;expirydate&quot;, {rid:&apos;{!Opportunity.Id}&apos;}); 

if(checkExpiry == &apos;ok&apos;){ 
retStr = sforce.apex.execute(&quot;rrde.AssignSignleRecordctrlr&quot;, &quot;assignopp&quot;, {oppId:&apos;{!Opportunity.Id}&apos;}); 
alert(&apos;Re-assignment initiated.\n NOTE : Record ownership will change only if all criteria meet.&apos;); 
window.top.location.reload(); 
} 
else { 
alert(&apos;Your RRD licence has expired, Please contact us sales@mirketa.com to renew your license.&apos;); 
}</url>
    </webLinks>
