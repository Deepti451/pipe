    <validationRules>
        <fullName>rrde__Record_assign_limit</fullName>
        <active>true</active>
        <errorConditionFormula>rrde__Record_assign_limit__c == 0  ||  rrde__Record_assign_limit__c &lt; rrde__Assigned_records_current_cycle__c ||
 rrde__Allowed_Number_of_Open_Cases__c &lt;
 rrde__Total_Assigned_Cases_f__c</errorConditionFormula>
        <errorDisplayField>rrde__Record_assign_limit__c</errorDisplayField>
        <errorMessage>Record assign limit should be greater than 0 and the number of assigned records in current cycle on Round Robin User.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rrde__User_Name_Required</fullName>
        <active>true</active>
        <errorConditionFormula>(rrde__Round_Robin_User_Name__c == null &amp;&amp; 
(rrde__Team__r.RecordType.Name ==&apos;Lead Criteria&apos; || rrde__Team__r.RecordType.Name ==&apos;Case Criteria&apos;))</errorConditionFormula>
        <errorDisplayField>rrde__Round_Robin_User_Name__c</errorDisplayField>
        <errorMessage>Please select Round Robin User</errorMessage>
    </validationRules>
