    <fields>
        <fullName>rrde__To_be_assigned_records__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( isblank(rrde__Allowed_Number_of_Open_Cases__c) , 
rrde__Record_assign_limit__c - rrde__Assigned_records_current_cycle_front__c, 

IF((rrde__Allowed_Number_of_Open_Cases__c - rrde__Total_Assigned_Cases__c )&gt;= 
(rrde__Record_assign_limit__c - rrde__Assigned_records_current_cycle__c), 
rrde__Record_assign_limit__c - rrde__Assigned_records_current_cycle__c, 
rrde__Allowed_Number_of_Open_Cases__c - rrde__Total_Assigned_Cases__c 
)
)</formula>
        <label>To be assigned records</label>
